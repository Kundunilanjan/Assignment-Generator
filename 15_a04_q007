#include <stdio.h>
#include <stdlib.h>
#include <string.h>



char *string_toupper(char string[])
{
    char ch;
    int length = strlen(string);
    for (int i=0;i<length;i++)
    {
        ch = string[i];   
        if (ch>='a' && ch <='z') ch = (char)((int)ch - 32);
        else continue;
        string[i] = ch;
    }

    char *ptr = string;
    return ptr;
}



int main()
{
    printf("ENTER NUMBER OF WORDS YOU WANT TO ENTER:\n");
    int n;
    int a,b;
    char *ptr;
    scanf("%d",&n);
   
    char string_array[n][100];

    // inputting the strings.
    printf("ENTER THE STRINGS:\n");
    for(int i=1;i<=n;i++) scanf("%s",string_array[i-1]);
   

    // converting the strings to uppercase.
    for(int i=0;i<n;i++)
    {
        ptr = string_toupper(string_array[i]);
        strcpy(string_array[i],ptr);
    }
   
    printf("\n");
    // displaying strings converted to uppper case.
    printf("THE STRINGS CONVERTED TO UPPER CASE ARE:\n");
    for(int i=0;i<n;i++) printf("%s\n",string_array[i]);



    /*if (strcmp(string_array[a-1],string_array[b-1])>0) printf("String %s is greater than %s.\n",string_array[a-1],string_array[b-1]);
    else if (strcmp(string_array[a-1],string_array[b-1])<0) printf("String %s is greater than %s.\n",string_array[b-1],string_array[a-1]);
    else printf("Both %s and %s are equal.\n",string_array[a-1],string_array[b-1]);*/
    printf("\n");
    for(int i=0;i<n-1;i++)
    {
        for(int j=i+1;j<n;j++)
        {
            if (strcmp(string_array[i],string_array[j])>0) printf("String %s is greater than %s.\n",string_array[i],string_array[j]);
            else if (strcmp(string_array[i],string_array[j])<0) printf("String %s is greater than %s.\n",string_array[j],string_array[i]);
            else printf("Both %s and %s are equal.\n",string_array[i],string_array[j]);
        }
    }
           
return 0;
}
